{
  "entrypoint": "index.html",
  "shell": "src/ktlg-app.html",
  "fragments": [
    "src/ktlg-dashboard.html",
    "src/ktlg-devices.html",
    "src/ktlg-view404.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*.html",
    "src/**/*.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
